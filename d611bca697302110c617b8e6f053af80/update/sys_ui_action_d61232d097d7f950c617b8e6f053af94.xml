        <name>Issue PAM</name>
        <onclick/>
        <order>100</order>
        <script><![CDATA[if (current) {
    // Creating RTP record
    var rtp = new GlideRecord("x_935283_pia_rtp");
    rtp.newRecord();
    rtp.project = current.sys_id;
    rtp.update();

    // Creating PIA record
    var pia = new GlideRecord("x_935283_pia_privacy_impact_assessment");
    pia.newRecord();
    pia.project = current.sys_id;
    pia.phase = 'initiate';
    pia.update();

    // Update pam field
    var pam = new GlideRecord("x_935283_pia_pam");
    pam.addQuery('name_of_project', current.sys_id);
    pam.query();
    if (pam.next()) {
        current.pam = pam.sys_id;
    }

    current.phase = "pam_issued";
    current.rtp = rtp.sys_id;
    current.update();
    //action.setReturnURL(current);


}]]></script>
